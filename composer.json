{
    "name": "galiais/flysystem-ipfs",
    "description": "Flysystem adapter for the IPFS.",
    "require": {
        "league/flysystem": "^3.0",
        "chenjia404/php-ipfs-api": "dev-master",
        "guzzlehttp/guzzle": "^7.8.0",
        "openwse/ipfs-api": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.4.0",
        "jetbrains/phpstorm-attributes": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "GALIAIS\\Flysystem\\IPFS\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GALIAIS\\Flysystem\\IPFS\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "GALIAIS",
            "email": "i@galiais.com"
        }
    ],
    "version": "1.0.0",
    "license": "MIT",
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "scripts-descriptions": {
        "test": "Run all tests."
    }
}
